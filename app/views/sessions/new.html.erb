<head>
<link href="https://fonts.googleapis.com/css?family=Bungee+Hairline|Bungee+Shade|Monoton" rel="stylesheet">
</head>

<div class="center-block">
<h1>Oye!</h1>
<h3>Sign In</h3>
<form>
<%= form_tag sessions_path do %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <button type="submit" class="btn btn-default"><%= submit_tag "Log In" %></button>
<p id="alert"><%= alert %></p>
<% end %>
</form>
</div>

